extends pug/layout.pug
include pug/mixins.pug

block page
  title Climbing portfolio
  meta(name='description' content='Fabian Poels climbing pictures portfolio')

block navigation
  +navigation('climbing')
  
block content
  article#gallery
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_thomasprenn_menhir.jpg', 'images/portfolio/climbing/fabianpoels_thomasprenn_menhir_thumb.jpg', 'Thomas Prenn on Menhir, Passo Gardena (ITALY)', 361, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_laste.jpg', 'images/portfolio/climbing/fabianpoels_laste_thumb.jpg', 'Climbing in Laste, (ITALY)', 809, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_roccadicorno_rombodivento.jpg', 'images/portfolio/climbing/fabianpoels_roccadicorno_rombodivento_thumb.jpg', 'Rombo di vento at Rocca di corno, Finale (ITALY)', 361, 540)
    +galleryItemWithThumb('images/stories/lumignano/fabianpoels_lumignano_codyroth_boomerang_01.jpg', 'images/stories/lumignano/fabianpoels_lumignano_codyroth_boomerang_01_thumb.jpg', 'Cody Roth sending Boomerang, Lumignano (ITALY)', 361, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_arco_belvedere.jpg', 'images/portfolio/climbing/fabianpoels_arco_belvedere_thumb.jpg', 'Crag Belvedere (near Arco), with a wonderful view of lake Garda (ITALY)', 809, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_wurzjoch.jpg', 'images/portfolio/climbing/fabianpoels_wurzjoch_thumb.jpg', 'Wurzjoch - Passo delle Erbe (ITALY)', 809, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_ciastlins.jpg', 'images/portfolio/climbing/fabianpoels_ciastlins_thumb.jpg', 'Stardust we are, Cascate di Ciastlins (ITALY)', 361, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_latelier_flo.jpg', 'images/portfolio/climbing/fabianpoels_latelier_flo_thumb.jpg', 'Crimping hard on the suberp rock at l\'atelier, vallon du Fournel (FRANCE)', 809, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_rienztal.jpg', 'images/portfolio/climbing/fabianpoels_rienztal_thumb.jpg', 'Rienztal (ITALY)', 809, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_freyr.jpg', 'images/portfolio/climbing/fabianpoels_freyr_thumb.jpg', 'Ruben Warrinnier in Freyr, (BELGIUM)', 361, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_matthiasschiestl_stohlwond_bockhaus.jpg', 'images/portfolio/climbing/fabianpoels_matthiasschiestl_stohlwond_bockhaus_thumb.jpg', 'Matthias Schiestl in Jeepers Creepers, Bockhaus, Stohlwond (ITALY)', 354, 540)
    
block append scripts
  script(src='assets/js/jquery.fancybox.min.js')
  script(src='assets/js/jquery.justifiedGallery.min.js')
  script(src='js/imagegallery.js')
