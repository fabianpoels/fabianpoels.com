extends pug/layout.pug
include pug/mixins.pug

block page
  title Fabian Poels climbing and outdoor photography
  meta(name='description' content='Fabian Poels climbing pictures portfolio')

block navigation
  +navigation('climbing')
  
block content
  article#gallery
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_ciastlins.jpg', 'images/portfolio/climbing/fabianpoels_ciastlins_thumb.jpg', 'Stardust we are, Cascate di Ciastlins (ITALY)', 361, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_latelier_flo.jpg', 'images/portfolio/climbing/fabianpoels_latelier_flo_thumb.jpg', 'Crimping hard on the suberp rock at l\'atelier, vallon du Fournel (FRANCE)', 809, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_rienztal.jpg', 'images/portfolio/climbing/fabianpoels_rienztal_thumb.jpg', 'Rienztal (ITALY)', 809, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_matthiasschiestl_stohlwond_bockhaus.jpg', 'images/portfolio/climbing/fabianpoels_matthiasschiestl_stohlwond_bockhaus_thumb.jpg', 'Matthias Schiestl in Jeepers Creepers, Bockhaus, Stohlwond (ITALY)', 354, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_stohlwond_bockhaus_belayer.jpg', 'images/portfolio/climbing/fabianpoels_stohlwond_bockhaus_belayer_thumb.jpg', 'Bockhaus, Stohlwond (ITALY)', 808, 540)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_ailefroide_boulder.jpg', 'images/portfolio/climbing/fabianpoels_ailefroide_boulder_thumb.jpg', 'Bouldering in Ailefroide (FRANCE)', 361, 540)
    +galleryItem('images/portfolio/climbing/fabianpoels_20180218_hybride_la_nausee_web.jpg', 'Crimping hard in Hybride, La Nausee (BELGIUM)', 802, 1200)
    +galleryItemWithThumb('images/portfolio/climbing/fabianpoels_yves_freyr_manhattantransfer.jpg', 'images/portfolio/climbing/fabianpoels_yves_freyr_manhattantransfer_thumb.jpg', 'Local hero Yves cruising through Manhattan Transfer, Freyr (BELGIUM)', 853, 540)
    +galleryItem('images/portfolio/climbing/fabianpoels_20170805_hybride_lanausee_web.jpg', 'Small crimps and bad feet in Hybride, La Nausee (BELGIUM)', 1912, 1200)
    +galleryItem('images/portfolio/climbing/fabianpoels_20171105_ilcagasotto_gumbi_toirano_web.jpg', 'Slopy sidepulls in the crux of Il Cagasotto, sector Gumbi, Toirano (ITALY)', 1917, 1200)
    +galleryItem('images/portfolio/climbing/fabianpoels_20171103_taccheticche_bausu_web.jpg', 'Abrasive limestone in Bausu Centrale, Val Pennavaire (ITALY)', 2384, 1200)
    
block append scripts
  script(src='assets/js/jquery.fancybox.min.js')
  script(src='assets/js/jquery.justifiedGallery.min.js')
  script(src='js/imagegallery.js')
